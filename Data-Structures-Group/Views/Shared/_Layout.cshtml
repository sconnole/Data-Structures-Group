<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Data Structures</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="https://www.w3schools.com/w3css/4/w3.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/site.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/site.js"></script>
</head>
<body>
    <div class="navbar">
        <div class="navbar-header">
            <ul>
                <li> @Html.ActionLink("Queue", "Index", "Queue", new { area = "" }, new { @class = "navbar-brand w3-bar-item w3-button" })</li>
                <li> @Html.ActionLink("Stack", "Index", "Stack", new { area = "" }, new { @class = "navbar-brand w3-bar-item w3-button" })</li>
                <li> @Html.ActionLink("Dictionary", "Index", "Dictionary", new { area = "" }, new { @class = "navbar-brand w3-bar-item w3-button" })</li>
                <li> @Html.ActionLink("Exit", "Exit", "Home", new { area = "" }, new { @class = "navbar-brand w3-bar-item w3-button" })</li>
            </ul>
        </div>
    </div>

    <div class="body-content">
        @RenderBody()
        <footer>
            <p class="bottom_right">&copy; @DateTime.Now.Year - Data Structures</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>